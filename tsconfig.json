{
    "compilerOptions": {
	"target": "es2016",
	"module": "commonjs",
	"rootDirs": [
	    "src",
	    "node_modules"
	],
	"outDir": "out",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": false,
	"strictNullChecks": false,
	"skipLibCheck": true,
    },
    "exclude": ["node_modules"]
}
