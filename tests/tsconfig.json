{
    "compilerOptions": {
	"target": "es2016",
	"module": "commonjs",
	"rootDirs": [
	    "../src",
	    "../node_modules"
	],
	"outDir": "../out",
	"esModuleInterop": true,
	"forceConsistentCasingInFileNames": true,
	"strict": true,
	"strictNullChecks": false,
	"skipLibCheck": true,
    },
    "include": ["../src"],
    "exclude": ["../node_modules"]
}
